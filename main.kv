<ElementCard@MDCard>:
    image: ''
    name: ''
    t : 'detail'
    sip_x: 130
    price  : '  Buy  '
    font : '20dp'
    sip_y: 90
    size_hint_x: None
    width: "150dp"
    elevation: 0
    on_release: app.transition_to_detail(root.t,root.image, root.name)
    size_hint_y: None
    md_bg_color: 1, 1, 1, .5
    height: "200dp"
    padding: "15dp"
    spacing: "15dp"
    radius: [25]

    BoxLayout:
        orientation : 'vertical'
        size_hint_y: None
        height: "160dp"
        spacing : '17dp'
        AsyncImage:
            source: root.image
            size_hint_x: None
            size_hint_y: None
            width: dp(root.sip_x)
            height: dp(root.sip_y)

        MDLabel:
            text: root.name
            halign: 'center'
            font_size: root.font
    
        MDRoundFlatIconButton:
            text:root.price
            icon : 'shopping'
            icon_size : '20dp'
            font_size : '20sp'
            on_release: app.buyd(root.name)
            theme_text_color: "Secondary" 
