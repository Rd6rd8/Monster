<Home>:
    name: 'home'
    Screen:
        AsyncImage:
            source: "/storage/emulated/0/Monster _ New Card/Assests/bg.png"
            allow_stretch: True
            opacity : .5
    BoxLayout:
        orientation: "vertical"
        padding : 0,0,0,'700dp'
        MDTopAppBar:
            BoxLayout:
                orientation: "horizontal"
                padding: [0, '30dp', 0, 0]
