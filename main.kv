        MDTopAppBar:
            icon : 'sword-cross'
            type:'bottom'
            elevation : 0

            BoxLayout:
                orientation: "horizontal"
                spacing: "40dp"  
                padding: [0, '30dp', 0, "30dp"]
                IconLeftWidget:
                    icon: "account"
                    theme_text_color: "Custom"
